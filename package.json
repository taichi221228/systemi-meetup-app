{
	"name": "my-qwik-empty-starter",
	"description": "App with Routing built-in ready to create your app",
	"engines": {
		"node": "^18.17.0 || ^20.3.0 || >=21.0.0"
	},
	"engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
	"private": true,
	"trustedDependencies": ["sharp"],
	"trustedDependencies-annotation": "Needed for bun to allow running install scripts",
	"type": "module",
	"scripts": {
		"build": "qwik build",
		"build.client": "vite build",
		"build.preview": "vite build --ssr src/entry.preview.tsx",
		"build.types": "tsc --incremental --noEmit",
		"deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
		"dev": "vite --mode ssr",
		"dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
		"fmt": "concurrently -n \"prettier,biome\" -c \"#F8BC45,#61A5FA\"  \"prettier --write .\" \"bunx @biomejs/biome format --write .\"",
		"fmt.check": "concurrently -n \"prettier,biome\" -c \"#F8BC45,#61A5FA\"  \"prettier --check .\" \"bunx @biomejs/biome format .\"",
		"lint": "concurrently -n \"eslint,biome\" -c \"#4831BD,#61A5FA\" \"ESLINT_USE_FLAT_CONFIG=false eslint -c \\\"./eslint.config.cjs\\\" \\\"src/**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx}\\\"\" \"bunx @biomejs/biome lint .\"",
		"lint.fix": "concurrently -n \"eslint,biome\" -c \"#4831BD,#61A5FA\" \"ESLINT_USE_FLAT_CONFIG=false eslint -c \\\"./eslint.config.cjs\\\" --fix \\\"src/**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx}\\\"\" \"bunx @biomejs/biome lint --write .\"",
		"prebuild.pandacss": "panda codegen --silent",
		"prepare": "husky",
		"preview": "qwik build preview && vite preview --open",
		"start": "vite --open --mode ssr",
		"qwik": "qwik"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.2",
		"@builder.io/qwik": "^1.5.7",
		"@builder.io/qwik-city": "^1.5.7",
		"@builder.io/qwik-react": "0.5.0",
		"@builder.io/vite-plugin-macro": "~0.0.7",
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@fontsource-variable/inter": "^5.0.18",
		"@pandacss/dev": "^0.36.1",
		"@react-three/drei": "^9.107.0",
		"@react-three/fiber": "^8.16.8",
		"@react-three/postprocessing": "^2.16.2",
		"@types/eslint": "^8.56.10",
		"@types/node": "^20.12.7",
		"@types/react": "^18.2.28",
		"@types/react-dom": "^18.2.13",
		"@types/three": "^0.165.0",
		"@typescript-eslint/eslint-plugin": "^7.7.1",
		"@typescript-eslint/parser": "^7.7.1",
		"better-typescript-lib": "^2.7.0",
		"concurrently": "^8.2.2",
		"eslint": "^8.57.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-import-access": "^2.2.1",
		"eslint-plugin-qwik": "^1.5.7",
		"husky": "^9.0.11",
		"postcss-load-config": "^6.0.1",
		"prettier": "^3.2.5",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"three": "^0.165.0",
		"typescript": "5.4.5",
		"undici": "*",
		"vite": "^5.2.10",
		"vite-tsconfig-paths": "^4.2.1"
	}
}
